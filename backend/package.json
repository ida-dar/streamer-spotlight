    "helmet": "^7.0.0",
    "http-proxy-middleware": "^2.0.6",
    "mocha": "^10.2.0",
    "mongodb": "^5.6.0",
    "mongoose": "^7.3.1",
    "onchange": "^7.1.0",
    "request-ip": "^3.3.0",
    "socket.io": "^4.7.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
